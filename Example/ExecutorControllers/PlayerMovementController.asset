    methodAttributeLine: "\t[Service(\"battle/ProcessAttack\")]\n"
    methodHeadLine: "\tprivate void on_ProcessAttack_service(Service service, ServiceExecuteType
      type)\n"
    methodBodyLines: "\t\tif (Input.GetMouseButtonDown(0))\r\n\t\t{\r\n\t\t\tSetTrigger(\"Attack\");\r\n\t\t}\n"
    linePosition: 74
    independentGenerate: 0
    isDeleted: 0
  - methodType: 1
    targetName: battle_look_to_target/FaceToTarget
    targetDescription: 
    methodAttributeLine: "\t[Service(\"battle_look_to_target/FaceToTarget\")]\n"
    methodHeadLine: "\tprivate void on_FaceToTarget_service(Service service, ServiceExecuteType
      type)\n"
    methodBodyLines: "\t\tVector3 tForward = (lookTarget.position - gameObject.transform.position).normalized;\r\n\t\ttForward.y
      = 0;\r\n\t\tgameObject.transform.forward = Vector3.Lerp(gameObject.transform.forward,
      tForward, Time.deltaTime * 20);\n"
    linePosition: 83
    independentGenerate: 1
    isDeleted: 0
