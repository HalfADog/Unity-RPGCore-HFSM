  methodBlocks:
  - methodType: 1
    targetName: Root/ProcessInput
    targetDescription: 
    methodAttributeLine: "\t[Service(\"Root/ProcessInput\")]\n"
    methodHeadLine: "\tprivate void on_ProcessInput_service(Service service, ServiceExecuteType
      type)\n"
    methodBodyLines: "\t\tmoveVec.x = (Input.GetKey(KeyCode.A) ? -1 : 0) + (Input.GetKey(KeyCode.D)
      ? 1 : 0);\r\n\t\tmoveVec.y = (Input.GetKey(KeyCode.S) ? -1 : 0) + (Input.GetKey(KeyCode.W)
      ? 1 : 0);\r\n\t\tmoveVec = moveVec.normalized;\r\n\t\tSetBool(\"IsIdle\", moveVec.sqrMagnitude
      == 0);\r\n\t\tSetBool(\"IsWalk\", moveVec.sqrMagnitude != 0);\r\n\t\tSetBool(\"IsRun\",
      Input.GetKey(KeyCode.LeftShift));\r\n\t\tif (moveVec.sqrMagnitude != 0)\r\n\t\t{\r\n\t\t\tif
      (!GetBool(\"IsLookToTarget\"))\r\n\t\t\t{\r\n\t\t\t\tgameObject.transform.forward
      = Vector3.Lerp(gameObject.transform.forward, new Vector3(moveVec.x, 0, moveVec.y),
      Time.deltaTime * 10);\r\n\t\t\t}\r\n\t\t}\r\n\t\tif (Input.GetKeyDown(KeyCode.Tab))\r\n\t\t{\r\n\t\t\tSetBool(\"IsOnBattle\",
      !GetBool(\"IsOnBattle\"));\r\n\t\t}\n"
    linePosition: 20
    independentGenerate: 1
    isDeleted: 0
  - methodType: 1
    targetName: Root/CheckGetHit
    targetDescription: 
    methodAttributeLine: "\t[Service(\"Root/CheckGetHit\")]\n"
    methodHeadLine: "\tprivate void on_CheckGetHit_service(Service service, ServiceExecuteType
      type)\n"
    methodBodyLines: "\t\tif (playerManager.beAttack)\r\n\t\t{\r\n\t\t\tif (executeState
      != \"roll\" && executeState != \"dodge\") SetTrigger(\"GetHit\");\r\n\t\t\tplayerManager.beAttack
      = false;\r\n\t\t}\n"
    linePosition: 42
    independentGenerate: 0
    isDeleted: 0
  - methodType: 1
    targetName: normal/ProcessTalk
    targetDescription: 
    methodAttributeLine: "\t[Service(\"normal/ProcessTalk\")]\n"
    methodHeadLine: "\tprivate void on_ProcessTalk_service(Service service, ServiceExecuteType
      type)\n"
    methodBodyLines: "\t\tif (Input.GetKeyDown(KeyCode.T))\r\n\t\t{\r\n\t\t\tSetTrigger(\"Talk\");\r\n\t\t}\n"
    linePosition: 52
    independentGenerate: 1
    isDeleted: 0
  - methodType: 1
    targetName: battle/ProcessRollOrDodge
    targetDescription: 
    methodAttributeLine: "\t[Service(\"battle/ProcessRollOrDodge\")]\n"
    methodHeadLine: "\tprivate void on_ProcessRollOrDodge_service(Service service,
      ServiceExecuteType type)\n"
    methodBodyLines: "\t\tif (Input.GetMouseButtonDown(1))\r\n\t\t{\r\n\t\t\tSetTrigger(\"RollOrDodge\");\r\n\t\t}\r\n\t\tif
      (Input.GetKeyDown(KeyCode.V))\r\n\t\t{\r\n\t\t\tSetBool(\"IsLookToTarget\",
      !GetBool(\"IsLookToTarget\"));\r\n\t\t}\n"
    linePosition: 61
    independentGenerate: 1
    isDeleted: 0
  - methodType: 1
    targetName: battle/ProcessAttack
